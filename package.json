{
    "name": "n-app-todo",
    "version": "0.0.1",
    "description": "n app todo app",
    "main": "src/server/server.js",
    "scripts": {
        "ts-compile": "tsc -p tsconfig.server.json",
        "ts-compile-watch": "npm run ts-compile -- -w",
        "ts-lint": "tslint -p tsconfig.server.json -c tslint.json",
        "ts-build": "npm run ts-compile && npm run ts-lint",
        "ts-compile-client": "tsc -p tsconfig.client.json",
        "ts-lint-client": "tslint -p tsconfig.client.json -c tslint.json",
        "ts-build-client": "npm run ts-compile-client && npm run ts-lint-client",
        "webpack-build": "webpack",
        "webpack-build-watch": "npm run webpack-build -- -w",
        "prestatic-build": "npm run ts-build",
        "static-build": "node ./compress-static.js",
        "nodemon": "nodemon --ignore test/ --ignore src/client/ --ignore src/sdk/ --inspect=8101 --require source-map-support/register ./src/server/server.js package.name=web-app",
        "predev": "npm run ts-build",
        "dev": "concurrently \"npm:ts-compile-watch\" \"npm:nodemon\"",
        "pretest": "npm run ts-build",
        "test": "mocha --timeout 200000 --reporter spec --ui tdd --require ts-node/register \"./test/**/*.test.js\" || true",
        "pretest-ci": "npm run pretest",
        "test-ci": "mocha --reporter spec --ui tdd --require ts-node/register \"./test/**/*.test.js\"",
        "clean-src": "find  ./src -type f -name \"*.js\" -not -path \"*/3rd-party/*\" -delete -o -name \"*.map\" -not -path \"*/3rd-party/*\" -delete -o -name \".DS_Store\" -delete && rm -f *.tsbuildinfo",
        "clean-test": "find ./test -name \"*.js\" -delete -o -name \"*.map\" -delete && rm -f *.tsbuildinfo",
        "build": "npm run ts-build && npm run static-build && env=prod npm run webpack-build",
        "start": "node ./src/server/server.js package.name=web-app"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/shreymahendru/n-app-todo.git"
    },
    "keywords": [
        "n-app",
        "todo",
        "web",
        "app"
    ],
    "author": "Shrey Mahendru",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/shreymahendru/n-app-todo/issues"
    },
    "homepage": "https://github.com/SofDevInc/n-app-todo#readme",
    "devDependencies": {
        "@babel/core": "^7.16.0",
        "@babel/preset-env": "^7.16.0",
        "@types/jquery": "^3.5.8",
        "@types/mocha": "^8.2.2",
        "@types/node": "^14.14.37",
        "autoprefixer": "^10.4.0",
        "babel-loader": "^8.2.3",
        "btoa": "^1.2.1",
        "clean-webpack-plugin": "^4.0.0",
        "compression-webpack-plugin": "^9.0.0",
        "concurrently": "^6.4.0",
        "css-loader": "^6.5.1",
        "css-minimizer-webpack-plugin": "^3.1.3",
        "enhanced-resolve": "^5.8.3",
        "fibers": "^5.0.0",
        "file-loader": "^6.2.0",
        "fork-ts-checker-webpack-plugin": "^6.4.0",
        "hash-sum": "^2.0.0",
        "html-loader": "^3.0.1",
        "html-webpack-plugin": "^5.5.0",
        "html-webpack-tags-plugin": "^3.0.2",
        "imagemin": "^7.0.1",
        "imagemin-gifsicle": "^7.0.0",
        "imagemin-jpegtran": "^7.0.0",
        "imagemin-mozjpeg": "^9.0.0",
        "imagemin-pngquant": "^9.0.2",
        "imagemin-svgo": "^9.0.0",
        "imagemin-webp": "^6.0.0",
        "loader-utils": "^2.0.0",
        "mini-css-extract-plugin": "^2.4.4",
        "mocha": "^8.3.2",
        "node-sass": "^6.0.1",
        "nodemon": "^2.0.15",
        "postcss": "^8.3.11",
        "postcss-flexbugs-fixes": "^5.0.2",
        "postcss-loader": "^6.2.0",
        "sass": "^1.43.4",
        "sass-loader": "^12.3.0",
        "script-loader": "^0.7.2",
        "sharp": "^0.30.1",
        "source-map-loader": "^3.0.0",
        "style-loader": "^3.3.1",
        "terser-webpack-plugin": "^5.2.5",
        "ts-loader": "^9.2.6",
        "ts-node": "^10.4.0",
        "tslint": "^6.1.3",
        "tslint-loader": "^3.5.4",
        "typescript": "4.4.4",
        "typescript-tslint-plugin": "^1.0.1",
